<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.community.yuequ"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="com.community.yuequ.YQApplication"
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.community.yuequ.gui.MainActivity"
            android:theme="@style/YQAppTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.community.yuequ.gui.ImageActivity"
            android:theme="@style/YQAppTheme" >
        </activity>
        <activity
            android:name="com.community.yuequ.gui.VideoGroupActivity"
            android:label="@string/title_activity_video_group"
            android:theme="@style/YQAppTheme" >
        </activity>
        <activity
            android:name="com.community.yuequ.gui.VideoListActivity"
            android:theme="@style/YQAppTheme" >
        </activity>
    </application>

</manifest>